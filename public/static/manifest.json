{
  "author": "https://www.onekey.so",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "19": "images/icon-19.png",
      "32": "images/icon-32.png",
      "38": "images/icon-38.png",
      "64": "images/icon-64.png",
      "128": "images/icon-128.png",
      "512": "images/icon-512.png"
    },
    "default_title": "OneKey",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+M",
        "mac": "Alt+Shift+M",
        "chromeos": "Alt+Shift+M",
        "linux": "Alt+Shift+M"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/disable-console.js",
        "js/globalthis.js",
        "js/lockdown-install.js",
        "js/lockdown-run.js",
        "js/contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    },
    {
      "matches": [
        "*://connect.trezor.io/*/popup.html",
        "*://connect.onekey.so/*/popup.html",
        "*://connect.onekey.so/popup.html"
      ],
      "js": [
        "js/onekey-content-script.js"
      ]
    }
  ],
  "description": "__MSG_appDescription__ (## DEV_VERSION ##)",
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png",
    "512": "images/icon-512.png"
  },
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "http://localhost:8545/",
    "https://*.infura.io/",
    "*://*.onekey.so/",
    "activeTab",
    "webRequest",
    "*://*.eth/",
    "notifications",
    "webRequestBlocking"
  ],
  "short_name": "__MSG_appName__",
  "web_accessible_resources": [
    "phishing.html",
    "phishing_en.html"
  ],
  "version": "2.0.1",
  "externally_connectable": {
    "matches": [
      "https://metamask.io/*",
      "https://www.onekey.so/*"
    ],
    "ids": [
      "*"
    ]
  },
  "minimum_chrome_version": "63"
}